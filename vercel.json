{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "functions": {
    "api/*.js": {
      "runtime": "nodejs20.x"
    },
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    },
    "api/*.ts": {
      "runtime": "nodejs20.x"
    },
    "api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  }
}